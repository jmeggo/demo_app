<h1>Listing ussers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ussers.each do |usser| %>
      <tr>
        <td><%= usser.name %></td>
        <td><%= usser.email %></td>
        <td><%= link_to 'Show', usser %></td>
        <td><%= link_to 'Edit', edit_usser_path(usser) %></td>
        <td><%= link_to 'Destroy', usser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usser', new_usser_path %>
